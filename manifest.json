{
  "description": "Search domain informations from devtools-panels and submit picked domains to a server",
  "manifest_version": 2,
  "name": "domain-info-devtools-panels",
  "version": "1.0",
  "author": "tanmingxin",
  "homepage_url": "https://github.com/1121170088/domain-info-devtools-panels",
  "icons": {
    "48": "icons/star.png"
  },
  
  "background": {
    "scripts": ["background_scripts/background.js"]
  },
  
  "permissions": [
      "<all_urls>",
	  "storage"
  ],
  
  "devtools_page": "devtools/devtools-page.html"

}
